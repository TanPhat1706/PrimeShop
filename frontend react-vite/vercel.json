{
  "rewrites": [
    { "source": "/assets/(.*\\.(js|css|png|jpg|svg|woff2?))", "destination": "/assets/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*\\.js)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    },
    {
      "source": "/assets/(.*\\.css)",
      "headers": [
        { "key": "Content-Type", "value": "text/css" }
      ]
    }
  ]
}